<#list properties as pro>
	-- 创建表：${prefixTableDbName}
	CREATE TABLE IF NOT EXISTS ${prefixTableDbName} (
	<#list pro.columnList as pro2>
        <#if pro2.columnName == "id">
            id int(11) NOT NULL AUTO_INCREMENT<#if pro2_has_next>,</#if>  -- ${pro2.description}；（示例）${pro2.example}
        <#else>
            ${pro2.columnName} ${pro2.columnType}<#if pro2_has_next>,</#if>  -- ${pro2.description}；（示例）${pro2.example}
        </#if>
    </#list>
    ) DEFAULT CHARSET=utf8;

</#list>